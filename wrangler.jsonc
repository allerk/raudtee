{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "mock-ms-graph-mail-service",
	"main": ".svelte-kit/cloudflare/_worker.js",
	"compatibility_date": "2026-01-13",
	"assets": {
		"directory": ".svelte-kit/cloudflare/assets",
		"binding": "ASSETS"
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "d1-mail-service",
			"database_id": "local"
		}
	],
	"r2_buckets": [
		{
			"binding": "BUCKET",
			"bucket_name": "r2-mail-service"
		}
	]
}
